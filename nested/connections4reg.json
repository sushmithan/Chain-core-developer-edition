{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "connectionSettings": {
            "type": "object"
        }
    },
    "variables": {
        "gatewayNameReg1": "[parameters('connectionSettings').gatewayNameReg1]",
        "gatewayNameReg2": "[parameters('connectionSettings').gatewayNameReg2]",
        "gatewayNameReg3": "[parameters('connectionSettings').gatewayNameReg3]",
        "gatewayNameReg4": "[parameters('connectionSettings').gatewayNameReg4]",
        "gatewayNameReg5": "[parameters('connectionSettings').gatewayNameReg5]",
        "VPNSharedKey": "[parameters('connectionSettings').VPNSharedKey]",
        "location_1": "[parameters('connectionSettings').location_1]",
        "location_2": "[parameters('connectionSettings').location_2]",
        "location_3": "[parameters('connectionSettings').location_3]",
        "location_4": "[parameters('connectionSettings').location_4]",
        "location_5": "[parameters('connectionSettings').location_5]"
    },
    "resources": [
        {
            "name": "reg1-to-reg2",
            "type": "Microsoft.Network/connections",
            "apiVersion": "2016-03-30",
            "location": "[variables('location_1')]",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "virtualNetworkGateway1": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg1'))]"
                },
                "virtualNetworkGateway2": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg2'))]"
                },
                "connectionType": "Vnet2Vnet",
                "sharedKey": "[variables('VPNSharedKey')]"
            }
        },
        {
            "name": "reg2-to-reg1",
            "type": "Microsoft.Network/connections",
            "apiVersion": "2016-03-30",
            "dependsOn": [
                "Microsoft.Network/connections/reg1-to-reg2"
            ],
            "location": "[variables('location_2')]",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "virtualNetworkGateway1": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg2'))]"
                },
                "virtualNetworkGateway2": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg1'))]"
                },
                "connectionType": "Vnet2Vnet",
                "sharedKey": "[variables('VPNSharedKey')]"
            }
        },
        {
            "name": "reg1-to-reg3",
            "type": "Microsoft.Network/connections",
            "apiVersion": "2016-03-30",
            "location": "[variables('location_1')]",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "virtualNetworkGateway1": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg1'))]"
                },
                "virtualNetworkGateway2": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg3'))]"
                },
                "connectionType": "Vnet2Vnet",
                "sharedKey": "[variables('VPNSharedKey')]"
            }
        },
        {
            "name": "reg3-to-reg1",
            "type": "Microsoft.Network/connections",
            "apiVersion": "2016-03-30",
            "dependsOn": [
                "Microsoft.Network/connections/reg1-to-reg3"
            ],
            "location": "[variables('location_3')]",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "virtualNetworkGateway1": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg3'))]"
                },
                "virtualNetworkGateway2": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg1'))]"
                },
                "connectionType": "Vnet2Vnet",
                "sharedKey": "[variables('VPNSharedKey')]"
            }
        },
        {
            "name": "reg1-to-reg4",
            "type": "Microsoft.Network/connections",
            "apiVersion": "2016-03-30",
            "location": "[variables('location_1')]",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "virtualNetworkGateway1": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg1'))]"
                },
                "virtualNetworkGateway2": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg4'))]"
                },
                "connectionType": "Vnet2Vnet",
                "sharedKey": "[variables('VPNSharedKey')]"
            }
        },
        {
            "name": "reg4-to-reg1",
            "type": "Microsoft.Network/connections",
            "apiVersion": "2016-03-30",
            "dependsOn": [
                "Microsoft.Network/connections/reg1-to-reg4"
            ],
            "location": "[variables('location_4')]",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "virtualNetworkGateway1": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg4'))]"
                },
                "virtualNetworkGateway2": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', variables('gatewayNameReg1'))]"
                },
                "connectionType": "Vnet2Vnet",
                "sharedKey": "[variables('VPNSharedKey')]"
            }
        }
    ]
}